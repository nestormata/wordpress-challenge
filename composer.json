{
    "name": "nestormata/wordpress-challenge",
    "description": "Challenge WP plugin for Inpsyde",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Nestor Mata Cuthbert",
            "email": "nestor.mata@gmail.com"
        }
    ],
    "require": {
        "mustache/mustache": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "Challenge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "brain/monkey": "2.*",
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
